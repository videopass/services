{
	"name": "@videopass/services",
	"version": "0.0.31",
	"description": "videopass services",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"build": "tsc",
		"dev": "NODE_ENV=development env-cmd nodemon",
		"test": "env-cmd jest --watch",
		"start": "concurrently  'npm run dev' 'npm run firestore' 'npm run seed'",
		"kill-emulator": "kill-port 4000 && kill-port 5001  && kill-port 5000",
		"seed": "ttab tsx watch ./dbseed.ts",
		"firestore": "ttab npm run kill-emulator && firebase emulators:start --inspect-functions --only firestore",
		"update": "ncu -u @videopass*/* && npm install",
		"deploy": "rm -r ./lib && tsc && bump && npm publish --access public"
	},
	"author": "videopass",
	"license": "ISC",
	"dependencies": {
		"@types/fs-extra": "^11.0.1",
		"@videopass/model": "^0.0.33",
		"axios": "^1.4.0",
		"date-fns": "^2.30.0",
		"date-fns-tz": "^2.0.0",
		"delay": "^6.0.0",
		"ethers": "^5.7.2",
		"firebase": "^9.22.1",
		"form-data": "^4.0.0"
	},
	"devDependencies": {
		"@types/jest": "^29.5.1",
		"@types/node": "^20.2.5",
		"@types/node-fetch": "^2.6.4",
		"env-cmd": "^10.1.0",
		"firebase-admin": "^11.8.0",
		"jest": "^29.5.0",
		"kill-port": "^2.0.1",
		"nock": "^13.3.1",
		"nodemon": "^2.0.22",
		"npm": "^9.6.7",
		"npm-check-updates": "^16.10.12",
		"strong-mock": "^8.0.1",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"tslint": "^6.1.2",
		"tsx": "^3.12.7",
		"ttab": "^0.8.0",
		"typescript": "^5.0.4",
		"version-bump-prompt": "^6.1.0"
	}
}
